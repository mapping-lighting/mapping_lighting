
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
<div class='container-fluid new_observation'>
  <h3>Statistics</h3>
  <span>Number of images collected</span>

  <table>
    <thead>

    <tr><th>Lighting Type</th><th>Count</th></tr>
    </thead>

  <% lighting_type_stats.each do |lighting_type, count| %>
    <tr><th><%= lighting_type.to_s.humanize %></th><td><%= count %></td></tr>
  <% end %>
  </table>

</div>
